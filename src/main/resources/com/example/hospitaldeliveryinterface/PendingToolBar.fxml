<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<ToolBar fx:id="PendingToolBarID" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" >
   <items>
      <Button fx:id="deliverPackageButton" mnemonicParsing="false" prefHeight="29.0" prefWidth="139.0" style="-fx-background-color: white;" text="Deliver Package">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Pane prefHeight="45.0" prefWidth="180.0" style="-fx-background-color: white;" HBox.hgrow="ALWAYS" />
      <Button fx:id="addNoteButton" mnemonicParsing="false" style="-fx-background-color: white;" text="Add Note">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="editDeliveryButton" mnemonicParsing="false" style="-fx-background-color: white;" text="Edit Delivery">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="newDeliveryButton" mnemonicParsing="false" onAction="#onNewDelivery" prefHeight="29.0" prefWidth="133.0" style="-fx-background-color: white;" text="+ New Delivery">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </items>
</ToolBar>
